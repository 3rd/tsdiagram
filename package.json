{
  "name": "tsdiagram",
  "version": "0.0.3",
  "repository": "https://github.com/3rd/tsdiagram",
  "description": "Create diagrams and plan your code with TypeScript.",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "@jspm/core": "^2.0.1",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@tisoap/react-flow-smart-edge": "^3.0.0",
    "@xyflow/react": "^12.0.3",
    "classnames": "^2.5.1",
    "dom-to-svg": "^0.12.2",
    "elkjs": "^0.9.3",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "monaco-vim": "^0.4.1",
    "nanoid": "^5.0.7",
    "path-browserify": "^1.0.1",
    "pathfinding": "^0.4.18",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.0.22",
    "source-map-js": "^1.2.0",
    "statelift": "^1.0.15",
    "ts-morph": "^23.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.12",
    "@types/pathfinding": "^0.0.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.4",
    "autoprefixer": "^10.4.19",
    "husky": "^9.1.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.4",
    "web-worker": "^1.3.0"
  }
}
