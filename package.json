{
  "name": "tsdiagram",
  "version": "0.0.3",
  "repository": "https://github.com/3rd/tsdiagram",
  "description": "Create diagrams and plan your code with TypeScript.",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@jspm/core": "^2.0.1",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@tisoap/react-flow-smart-edge": "^3.0.0",
    "classnames": "^2.3.2",
    "dom-to-svg": "^0.12.2",
    "elkjs": "^0.8.2",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "monaco-vim": "^0.4.1",
    "path-browserify": "^1.0.1",
    "pathfinding": "^0.4.18",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^0.0.63",
    "reactflow": "^11.10.1",
    "source-map-js": "^1.0.2",
    "statelift": "^1.0.13",
    "ts-morph": "^21.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/pathfinding": "^0.0.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^5.11.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "web-worker": "^1.2.0"
  }
}
