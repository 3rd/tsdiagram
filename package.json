{
  "name": "tsdiagram",
  "version": "0.0.3",
  "repository": "https://github.com/3rd/tsdiagram",
  "description": "Create diagrams and plan your code with TypeScript.",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@jspm/core": "^2.0.1",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@tisoap/react-flow-smart-edge": "^3.0.0",
    "classnames": "^2.5.1",
    "dom-to-svg": "^0.12.2",
    "elkjs": "^0.9.3",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "million": "^3.0.6",
    "monaco-vim": "^0.4.1",
    "nanoid": "^5.0.7",
    "path-browserify": "^1.0.1",
    "pathfinding": "^0.4.18",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^2.0.17",
    "reactflow": "^11.11.1",
    "source-map-js": "^1.2.0",
    "statelift": "^1.0.15",
    "ts-morph": "^22.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/pathfinding": "^0.0.9",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^1.5.0",
    "autoprefixer": "^10.4.19",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vitest": "^1.5.0",
    "web-worker": "^1.3.0"
  }
}
